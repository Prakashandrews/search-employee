    "@testing-library/user-event": "^7.1.2",
    "bootstrap": "^4.5.3",
    "enzyme-adapter-react-15": "^1.4.2",
    "react": "^16.14.0",
    "react-bootstrap": "^1.3.0",
    "react-dom": "^16.14.0",
    "react-scripts": "3.4.3",
